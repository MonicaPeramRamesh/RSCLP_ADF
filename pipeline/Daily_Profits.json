{
	"name": "Daily_Profits",
	"properties": {
		"activities": [
			{
				"name": "daily_profits_job",
				"type": "DatabricksJob",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"jobId": "348733752161984"
				},
				"linkedServiceName": {
					"referenceName": "AzureDatabricks_ls",
					"type": "LinkedServiceReference",
					"parameters": {
						"cluster_version": {
							"value": "@pipeline().globalParameters.cluster_version",
							"type": "Expression"
						},
						"cluster_nodetype": {
							"value": "@pipeline().globalParameters.cluster_nodetype",
							"type": "Expression"
						}
					}
				}
			},
			{
				"name": "daily_profits_alert",
				"type": "WebActivity",
