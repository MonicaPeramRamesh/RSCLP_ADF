{
	"name": "SqlServer_ls",
	"properties": {
		"parameters": {
			"db_username": {
				"type": "string"
			},
			"db_servername": {
				"type": "string"
			},
			"db_name": {
				"type": "string"
			},
			"keyvault_url": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "SqlServer",
		"typeProperties": {
			"server": "@{linkedService().db_servername}",
			"database": "@{linkedService().db_name}",
			"encrypt": "optional",
			"trustServerCertificate": false,
			"authenticationType": "SQL",
			"userName": "@{linkedService().db_username}",
			"password": {
				"type": "AzureKeyVaultSecret",
				"store": {
					"referenceName": "AzureKeyVault_ls",
					"type": "LinkedServiceReference",
					"parameters": {
						"keyvault_url": {
							"value": "@linkedService().keyvault_url",
							"type": "Expression"
						}
					}
				},
				"secretName": "rsclp-db-password"
			}
		}
	}
}